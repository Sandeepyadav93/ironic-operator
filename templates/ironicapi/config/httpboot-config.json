{
    "command": "/usr/sbin/httpd",
    "config_files": [
        {
            "source": "/var/lib/config-data/merged/httpboot-httpd.conf",
            "dest": "/etc/httpd/conf/httpd.conf",
            "owner": "root",
            "perm": "0644"
        }
    ],
    "permissions": [
        {
            "path": "/var/log/ironic",
            "owner": "ironic:ironic",
            "recurse": true
        },
        {
            "path": "/var/lib/ironic",
            "owner": "ironic:ironic",
            "recurse": true
        }
    ]
}